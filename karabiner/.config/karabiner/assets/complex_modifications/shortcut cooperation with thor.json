{
  "title": "Launch Apps cooperation with thor",
  "rules": [
    {
      "description": "right_contol+left_command+letters",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
                "mandatory": [
                  "right_control",
                  "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Safari.app'"
            }
          ]
        }
      ]
    }
  ]
}
