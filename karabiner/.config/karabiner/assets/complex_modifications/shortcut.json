{
  "title": "Launch Apps",
  "rules": [
    {
      "description": "right_contol+left_command+letters",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
                "mandatory": [
                  "right_control",
                  "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/WeChat.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
